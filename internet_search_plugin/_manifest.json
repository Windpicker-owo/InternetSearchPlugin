{
  "manifest_version": 1,
  "name": "麦麦联网搜索插件",
  "version": "1.0.0",
  "description": "提供联网搜索工具，使麦麦可以根据需要上网搜索信息",
  "author": {
    "name": "Windpicker_owo",
    "url": "https://github.com/Windpicker-owo"
  },
  "license": "GPL-v3.0-or-later",
  
  "host_application": {
    "min_version": "0.10.0"
  },
  "homepage_url": "https://github.com/MaiM-with-u/maibot",
  "repository_url": "https://github.com/MaiM-with-u/maibot",
  "keywords": ["tool", "search", "online"],
  
  "default_locale": "zh-CN",
  "locales_path": "_locales",
  
  "plugin_info": {
    "is_built_in": false,
    "components": [
      {
        "type": "tool",
        "name": "search_online",
        "description": "使用联网模型搜索"
      }
    ],
    "features": [
      "接入联网模型",
      "扩展麦麦能获取到的信息",
      "可供外部插件调用",
      "让麦麦更聪明"
    ]
  }
}